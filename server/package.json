{
    "name": "notestick",
    "version": "1.0.0",
    "description": "Notes Tick Music Game",
    "main": "main.js",
    "scripts": {
        "dev": "DEBUG=app:* node server.js",
        "winDev": "@powershell -Command $env:DEBUG='app:*';node server.js",
        "serve": "node server.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "make": "electron-forge make",
        "start": "electron-forge start",
        "package": "electron-forge package"
    },
    "keywords": [
        "music game",
        "tonejs"
    ],
    "author": "PorYoung, Zehong Chen",
    "license": "ISC",
    "dependencies": {
        "@tonejs/midi": "^2.0.28",
        "core-js": "^3.6.5",
        "cors": "^2.8.5",
        "debug": "^4.3.4",
        "electron-squirrel-startup": "^1.0.0",
        "express": "^4.18.2",
        "socket.io": "^4.7.2",
        "socket.io-client": "^4.4.0"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.4.1",
        "@electron-forge/maker-deb": "^6.4.1",
        "@electron-forge/maker-rpm": "^6.4.1",
        "@electron-forge/maker-squirrel": "^6.4.1",
        "@electron-forge/maker-zip": "^6.4.1",
        "@electron-forge/plugin-auto-unpack-natives": "^6.4.1",
        "electron": "^26.1.0"
    },
    "build": {
        "win": {
            "target": "squirrel",
            "icon": "static/images/favicon.ico"
        }
    },
    "config": {
        "forge": {
            "packagerConfig": {
                "asar": true,
                "icon": "static/images/favicon.ico",
                "name": "NotesTick",
                "overwrite": true,
                "packageManager": "npm"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                {
                    "name": "@electron-forge/plugin-auto-unpack-natives",
                    "config": {}
                }
            ]
        }
    }
}